{
  "manifest_version": 3,
  "name": "Odin Tabs",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "19": "icon.png",
    "38": "icon.png",
    "48": "icon.png",
    "96": "icon.png",
    "128": "icon.png"
  }
}